{
  "manifest_version": 3,
  "name": "Diccionari",
  "version": "1.0.0",
  "description": "Diccionari per estudiar",
  "icons": {
    "512": "images/dictionary.png"
  },
 
  "action":{ 
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting",
    "notifications",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}